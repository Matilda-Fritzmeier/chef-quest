<%= simple_form_for(caterer) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price_per_hour %>
  <%= f.input :cuisine , collection: Caterer::CUISINES%>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Create a Chef", class: "btn-flat"%>
<% end %>
